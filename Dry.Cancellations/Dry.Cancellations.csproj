<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Dmitry Nogin</Authors>
    <Product>Don't repeat yourself - operation cancellation</Product>
    <Description>Tiny library allowing to cancel operation execution tree.</Description>
    <Copyright>(c) Dmitry Nogin</Copyright>
    <PackageProjectUrl>https://github.com/dmitrynogin/drycancellation</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/dmitrynogin/drycancellation</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
